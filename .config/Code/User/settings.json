  "workbench.colorCustomizations": {
    "[Catppuccin Mocha]": {
      "editorIndentGuide.activeBackground1": "#45475A",
      "editorIndentGuide.background1": "#313244",
      // "editorLineNumber.foreground": "#6C7086",
      "editorRuler.foreground": "#313244"
    },
    // "gitDecoration.ignoredResourceForeground": "#949494aa",
    // "sideBar.foreground": "#b8b8b8",
    // "sideBar.foreground": "#9A9A9A"
  },
  "window.zoomLevel": 2,
  "git.allowForcePush": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "errorLens.fontFamily": "'Hack Nerd Font'",
  "errorLens.gutterIconsFollowCursorOverride": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "files.autoSave": "onFocusChange",
  "editor.semanticHighlighting.enabled": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": false
  },
  "gopls": {
    "ui.semanticTokens": true
  },
  "go.toolsManagement.autoUpdate": true,
  "extensions.autoUpdate": false,
  "remote.SSH.configFile": "/home/gbi/.ssh/config",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "window.customTitleBarVisibility": "auto",
  "update.mode": "manual",
  "terminal.integrated.defaultProfile.linux": "bash",
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.colorTheme": "Catppuccin Mocha",
  "editor.smoothScrolling": true,
  "editor.mouseWheelScrollSensitivity": 2,
  "workbench.activityBar.location": "top",
  "gitlens.views.worktrees.files.layout": "tree",
  "workbench.sideBar.location": "right",
