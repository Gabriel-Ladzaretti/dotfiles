    },
    {
      "backgroundColor": "#94e2d5",
      "color": "#11111b",
      "text": "FIXME6:"
    },
    {
      "backgroundColor": "#a6e3a1",
      "color": "#11111b",
      "text": "FIXME7:"
    }
  ],
  "update.mode": "manual",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 2,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[Catppuccin Mocha]": {
      "editorIndentGuide.activeBackground1": "#45475A",
      "editorIndentGuide.background1": "#313244",
      "editorInlayHint.background": "#313244",
      "editorInlayHint.foreground": "#cdd6f4",
      "editorInlayHint.parameterBackground": "default",
      "editorInlayHint.parameterForeground": "default",
      "editorInlayHint.typeBackground": "default",
      "editorInlayHint.typeForeground": "default",
      "editorRuler.foreground": "#313244",
      "statusBarItem.remoteBackground": "#11111b",
      "statusBarItem.remoteForeground": "#cdd6f4",
      "testing.iconFailed": "#f38ba8",
      "testing.iconPassed": "#a6e3a1",
      "testing.iconQueued": "#f9e2af",
      "testing.message.error.badgeBackground": "#f2cdcd",
      "testing.message.error.badgeBorder": "#313244",
      "testing.message.error.badgeForeground": "#11111b"
    }
  },
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.navigationControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.enableStickyScroll": true,
  "window.commandCenter": false,
  "window.menuBarVisibility": "toggle"
}
