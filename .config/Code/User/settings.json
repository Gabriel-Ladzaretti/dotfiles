{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.autoIndent": "advanced",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[go]": {
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 8
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "cSpell.useGitignore": false,
  "cSpell.userWords": [
    "golangci",
    "Ladzaretti",
    "nolint",
    "stdlib",
    "unmarshalling"
  ],
  "debug.console.fontSize": 14,
  "debug.inlineValues": "off",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "'JetBrainsMono Nerd Font'",
  "editor.fontSize": 14,
  "editor.fontVariations": false,
  "editor.fontWeight": "500",
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontSize": 14,
  "editor.inlayHints.padding": true,
  "editor.lineHeight": 1.8,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.rulers": [80, 110],
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "[Catppuccin Mocha]": {
      "rules": {
        "namespace": "#fab387",
        "parameter": "#cdd6f4",
        "parameter.definition": "#cdd6f4",
        "type": "#94e2d5",
        "type.defaultLibrary:go": "#f9e2af",
        "type.definition": "#f9e2af",
        "variable.defaultLibrary.readonly:go": "#eba0ac",
        "variable.readonly": "#f5e0dc"
      }
    },
    "enabled": true
  },
  "editor.smoothScrolling": true,
  "editor.tokenColorCustomizations": {
    "[Catppuccin Mocha]": {
      "textMateRules": [
        {
          "scope": [
            "meta.brace.square.ts",
            "meta.type.tuple.ts",
            "meta.type.annotation.ts",
            "meta.parameters.ts",
            "meta.function.ts",
            "source.ts"
          ],
          "settings": {
            "foreground": "#9399B2"
          }
        }
      ]
    },
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "explorer.autoReveal": true,
  "extensions.autoUpdate": false,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/Thumbs.db": true
  },
  "files.watcherExclude": {
    "**/vendor/**": true
  },
  "git.allowForcePush": true,
  "git.blame.editorDecoration.enabled": false,
  "go.addTags": {
    "options": "json=omitempty",
    "promptForTags": false,
    "tags": "json",
    "template": "",
    "transform": "snakecase"
  },
  "go.alternateTools": {
    "customFormatter": "golangci-lint"
  },
  "go.formatFlags": ["fmt", "--stdin"],
  "go.inlayHints.assignVariableTypes": false,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.lintFlags": ["--fast-only"],
  "go.lintTool": "golangci-lint",
  // "go.formatTool": "custom", // until golangci-lint v2 is supported in go-vscode
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "ui.semanticTokens": true
    // "ui.navigation.symbolScope": "workspace"
  },
  "Prettier-SQL.expressionWidth": 50,
  "Prettier-SQL.indentStyle": "standard",
  "Prettier-SQL.keywordCase": "upper",
  "remote.defaultExtensionsIfInstalledLocally": [],
  "remote.SSH.configFile": "/home/gbi/.ssh/config",
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/vendor": true
  },
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "alacritty",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontFamily": "'Hack Nerd Font'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": 500,
  "terminal.integrated.fontWeightBold": 600,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.tabs.enabled": false,
  "testing.gutterEnabled": true,
  "todohighlight.keywords": [
    {
      "backgroundColor": "#94e2d5",
      "color": "#11111b",
      "text": "TODO:"
    },
    {
      "backgroundColor": "#74c7ec",
      "color": "#11111b",
